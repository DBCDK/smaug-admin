{
  "name": "smaug-admin",
  "version": "0.0.1",
  "description": "An admin interface for Smaug webservice",
  "main": "src/index.js",
  "scripts": {
    "dev": "STANDARD_CACHE_TIME=1 run-p build:dev serve",
    "serve": "nodemon --exec node --no-stdin src/server/index.js",
    "build": "webpack --define process.env.NODE_ENV='\"production\"' --progress --colors -p",
    "build:dev": "webpack --progress --colors --watch -d",
    "lint": "eslint ./src --cache",
    "test": "LOG_LEVEL=off mocha src/utils/test.util.js --compilers js:babel-core/register --recursive src/**/__tests__/*",
    "testverbose": "LOG_LEVEL=debug mocha --compilers js:babel-core/register --recursive src/**/__tests__/*",
    "cy:run": "npx cypress run --project ./e2e",
    "cy:open": "npx cypress@latest open --project ./e2e",
    "prettier": "prettier --no-bracket-spacing --single-quote --write \"src/**/*.js\""
  },
  "keywords": [
    "admin",
    "ui",
    "smaug"
  ],
  "author": "ux",
  "license": "GPL-3.0",
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cypress": "^4.4.1",
    "dot-object": "^2.1.3",
    "google-auth-library": "^0.10.0",
    "googleapis": "^39.1.0",
    "ioredis": "^2.5.0",
    "jsdom": "^9.8.3",
    "jsoneditor": "5.5.10",
    "koa": "^2.11.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.0.0",
    "koa-session2": "^1.0.8",
    "koa-static": "^2.0.0",
    "lodash": "^4.17.15",
    "node-cache": "^5.1.1",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-dom": "^16.13.1",
    "sinon": "^1.17.6",
    "superagent": "^3.8.3",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.7",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^3.9.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.14.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^1.1.1",
    "prettier": "^1.17.0",
    "react-addons-test-utils": "^15.6.2",
    "sass-loader": "^4.0.2",
    "webpack": "^1.15.0"
  }
}
